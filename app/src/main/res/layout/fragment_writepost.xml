        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="396dp"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:src="@drawable/sqaure_lightyellow"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.126" />

        <ImageView
            android:id="@+id/imageView20"
            android:layout_width="254dp"
            android:layout_height="49dp"
            android:src="@drawable/write_post"
            app:layout_constraintBottom_toBottomOf="@+id/imageButton2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/menubar" />

        <TextView
            android:id="@+id/textView25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/laundrygothicbold"
            android:hyphenationFrequency="normal"
            android:text="카테고리 선택"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="194dp" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/laundrygothicbold"
            android:text="제    목"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="226dp" />


        <View
            android:id="@+id/categorySelectBackground"
            android:layout_width="248dp"
            android:layout_height="25dp"
            android:background="@drawable/border1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.352"
            app:layout_constraintStart_toEndOf="@+id/textView25"
            app:layout_constraintTop_toBottomOf="@+id/imageButton2"
            app:layout_constraintVertical_bias="0.03" />


        <Spinner
            android:id="@+id/spinner_postCategory"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="1dp"
            android:layout_weight="1"
            app:layout_constraintBottom_toBottomOf="@+id/categorySelectBackground"
            app:layout_constraintEnd_toEndOf="@+id/categorySelectBackground"
            app:layout_constraintHorizontal_bias="0.93"
            app:layout_constraintStart_toStartOf="@+id/categorySelectBackground"
            app:layout_constraintTop_toTopOf="@+id/categorySelectBackground"
            app:layout_constraintVertical_bias="0.0" />

        <View
            android:id="@+id/titleInputBackground"
            android:layout_width="299dp"
            android:layout_height="25dp"
            android:background="@drawable/border1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.45"
            app:layout_constraintStart_toEndOf="@+id/textView26"
            app:layout_constraintTop_toBottomOf="@+id/imageButton2"
            app:layout_constraintVertical_bias="0.081" />

        <EditText
            android:id="@+id/titleInput"
            android:layout_width="284dp"
            android:layout_height="21dp"
            android:background="@null"
            android:ems="10"
            android:fontFamily="@font/laundrygothic"

            android:inputType="text"
            android:textColor="@color/khaki"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/titleInputBackground"
            app:layout_constraintEnd_toEndOf="@+id/titleInputBackground"
            app:layout_constraintHorizontal_bias="0.41"
            app:layout_constraintStart_toEndOf="@+id/textView26"
            app:layout_constraintStart_toStartOf="@+id/titleInputBackground"
            app:layout_constraintTop_toTopOf="@+id/titleInputBackground"
            app:layout_constraintVertical_bias="0.2" />

        <ImageView
            android:id="@+id/imageView32"
            android:layout_width="364dp"
            android:layout_height="500dp"
            android:src="@drawable/strok_bigroundsqaure_green"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.551"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.781" />

        <EditText
            android:id="@+id/contents_input"
            android:layout_width="330dp"
            android:layout_height="430dp"
            android:background="@null"
            android:ems="10"
            android:fontFamily="@font/laundrygothic"

            android:inputType="text"
            android:textColor="@color/khaki"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView32"
            app:layout_constraintEnd_toEndOf="@+id/imageView32"
            app:layout_constraintHorizontal_bias="0.52"
            app:layout_constraintStart_toEndOf="@+id/textView26"
            app:layout_constraintStart_toStartOf="@+id/imageView32"
            app:layout_constraintTop_toTopOf="@+id/imageView32"
            app:layout_constraintVertical_bias="0.53" />

        <ImageButton
            android:id="@+id/write_finishBtn"
            android:layout_width="175dp"
            android:layout_height="72dp"
            android:background="@android:color/transparent"
            android:padding="10dp"
            android:scaleType="fitCenter"
            android:src="@drawable/write_finish_btn"
            tools:layout_editor_absoluteX="109dp"
            tools:layout_editor_absoluteY="741dp" />

