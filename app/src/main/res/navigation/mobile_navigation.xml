<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/startFragment">

    <fragment
        android:id="@+id/startFragment"
        android:name="com.example.dali_bike.startFragment"
        android:label="fragment_start"
        tools:layout="@layout/fragment_start" >
        <action
            android:id="@+id/action_startFragment_to_start2Fragment"
            app:destination="@id/start2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/start2Fragment"
        android:name="com.example.dali_bike.start2Fragment"
        android:label="fragment_start2"
        tools:layout="@layout/fragment_start2" >
        <action
            android:id="@+id/action_start2Fragment_to_start3Fragment"
            app:destination="@id/start3Fragment" />
    </fragment>
    <fragment
        android:id="@+id/start3Fragment"
        android:name="com.example.dali_bike.start3Fragment"
        android:label="fragment_start3"
        tools:layout="@layout/fragment_start3" >
        <action
            android:id="@+id/action_start3Fragment_to_start4Fragment"
            app:destination="@id/start4Fragment" />
    </fragment>
    <fragment
        android:id="@+id/start4Fragment"
        android:name="com.example.dali_bike.start4Fragment"
        android:label="fragment_start4"
        tools:layout="@layout/fragment_start4" />
</navigation>