            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.45"
            app:layout_constraintStart_toEndOf="@+id/textView26"
            app:layout_constraintTop_toBottomOf="@+id/imageButton2"
            app:layout_constraintVertical_bias="0.081" />


        <EditText
            android:id="@+id/categoryInput"
            android:layout_width="230dp"
            android:layout_height="21dp"
            android:background="@null"
            android:ems="10"
            android:fontFamily="@font/laundrygothic"

            android:inputType="text"
            android:textColor="@color/khaki"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/categorySelectBackground"
            app:layout_constraintEnd_toEndOf="@+id/categorySelectBackground"
            app:layout_constraintHorizontal_bias="0.41"
            app:layout_constraintStart_toEndOf="@+id/textView25"
            app:layout_constraintStart_toStartOf="@+id/categorySelectBackground"
            app:layout_constraintTop_toTopOf="@+id/categorySelectBackground"
            app:layout_constraintVertical_bias="0.2" />

        <EditText
            android:id="@+id/titleInput"
            android:layout_width="284dp"
            android:layout_height="21dp"
            android:background="@null"
            android:ems="10"
            android:fontFamily="@font/laundrygothic"

            android:inputType="text"
            android:textColor="@color/khaki"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/titleInputBackground"
            app:layout_constraintEnd_toEndOf="@+id/titleInputBackground"
            app:layout_constraintHorizontal_bias="0.41"
            app:layout_constraintStart_toEndOf="@+id/textView26"
            app:layout_constraintStart_toStartOf="@+id/titleInputBackground"
            app:layout_constraintTop_toTopOf="@+id/titleInputBackground"
            app:layout_constraintVertical_bias="0.2" />

        <ImageView
            android:id="@+id/imageView32"
            android:layout_width="364dp"
            android:layout_height="470dp"
            android:layout_marginBottom="3dp"
            android:src="@drawable/strok_bigroundsqaure_green"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.551"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/titleInputBackground"
            app:layout_constraintVertical_bias="0.110000014" />

        <EditText
            android:id="@+id/contents_input"
            android:layout_width="330dp"
            android:layout_height="430dp"
            android:background="@null"
            android:ems="10"
            android:fontFamily="@font/laundrygothic"
            android:gravity="top|start"
            android:inputType="textMultiLine"
            android:maxLines="10"
            android:scrollHorizontally="false"
            android:textColor="@color/khaki"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView32"
            app:layout_constraintEnd_toEndOf="@+id/imageView32"
            app:layout_constraintHorizontal_bias="0.52"
            app:layout_constraintStart_toEndOf="@+id/textView26"
            app:layout_constraintStart_toStartOf="@+id/imageView32"
            app:layout_constraintTop_toTopOf="@+id/imageView32"
            app:layout_constraintVertical_bias="0.53" />


        <ImageButton
            android:id="@+id/write_finishBtn"
            android:layout_width="175dp"
            android:layout_height="60dp"
            android:background="@android:color/transparent"
            android:padding="1dp"
            android:scaleType="fitCenter"
            android:src="@drawable/write_finish_btn"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView32"
            app:layout_constraintVertical_bias="0.25" />

